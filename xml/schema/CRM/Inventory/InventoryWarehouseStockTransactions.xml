<?xml version="1.0" encoding="iso-8859-1" ?>

<table>
  <base>CRM/Inventory</base>
  <class>InventoryWarehouseStockTransactions</class>
  <name>civicrm_inventory_warehouse_transactions</name>
  <comment>Warehouse Transaction</comment>
  <log>true</log>

  <field>
    <name>id</name>
    <type>int unsigned</type>
    <required>true</required>
    <comment>Unique Inventory Warehouse Stock Transactions ID</comment>
    <html>
      <type>Number</type>
    </html>
  </field>
  <primaryKey>
    <name>id</name>
    <autoincrement>true</autoincrement>
  </primaryKey>

  <field>
    <name>lot_id</name>
    <uniqueName>inventory_warehouse_transactions_lot_id</uniqueName>
    <title>Product Lot ID</title>
    <type>varchar</type>
    <length>100</length>
    <required>true</required>
    <export>true</export>
    <import>true</import>
    <add>5.63</add>
  </field>
  <field>
    <name>product_variant_id</name>
    <type>int unsigned</type>
    <comment>FK to Product Variant</comment>
  </field>
  <foreignKey>
    <name>product_variant_id</name>
    <table>civicrm_inventory_product_variant</table>
    <key>id</key>
    <onDelete>SET NULL</onDelete>
  </foreignKey>
  <field>
    <name>warehouse_id</name>
    <type>int unsigned</type>
    <comment>FK to Warehouse</comment>
  </field>
  <foreignKey>
    <name>warehouse_id</name>
    <table>civicrm_inventory_warehouse</table>
    <key>id</key>
    <onDelete>SET NULL</onDelete>
  </foreignKey>
  <field>
    <name>type</name>
    <uniqueName>inventory_warehouse_transactions_type</uniqueName>
    <title>Transaction Type</title>
    <type>varchar</type>
    <comment>IN = into location, OUT = OUT of location</comment>
    <length>100</length>
    <required>true</required>
    <export>true</export>
    <import>true</import>
    <add>5.63</add>
  </field>
  <field>
    <name>contact_id</name>
    <title>Operation performed By</title>
    <type>int unsigned</type>
    <comment>FK to Contact</comment>
  </field>
  <foreignKey>
    <name>contact_id</name>
    <table>civicrm_contact</table>
    <key>id</key>
    <onDelete>SET NULL</onDelete>
  </foreignKey>

  <field>
    <name>action_date</name>
    <title>Action Date</title>
    <type>datetime</type>
    <import>true</import>
    <headerPattern>/action(.?date)?/i</headerPattern>
    <dataPattern>/^\d{4}-?\d{2}-?\d{2} ?(\d{2}:?\d{2}:?(\d{2})?)?$/</dataPattern>
    <html>
      <type>Select Date</type>
      <formatType>activityDateTime</formatType>
    </html>
  </field>

</table>
